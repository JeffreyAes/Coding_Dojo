<h1>All songs</h1>
@model List<Music>
<table class="table table-striped">
    <tr>
        <th>Title</th>
        <th>Year</th>
        <th>Genre</th>
        <th>Artist</th>
        <th>Actions</th>
    </tr>
    @foreach (Music s in Model)
    {
        <tr>

            <td><a asp-action="ShowSong" asp-controller="Home" asp-route-musicId="@s.MusicId">@s.Title</a></td>
            <td><a asp-action="ShowSong" asp-controller="Home" asp-route-musicId="@s.MusicId">@s.Year</a></td>
            <td><a asp-action="ShowSong" asp-controller="Home" asp-route-musicId="@s.MusicId">@s.Genre</a></td>
            <td><a asp-action="ShowSong" asp-controller="Home" asp-route-musicId="@s.MusicId">@s.Artist</a></td>
            <td>
                <div class="d-flex gap-3">

                <form asp-action="DestroySong" asp-controller="Home" asp-route-musicId="@s.MusicId" method="post">
                    <input type="submit" value ="Delete" class="btn btn-danger">
                </form>
                <form asp-action="EditSong" asp-controller="Home" asp-route-musicId="@s.MusicId" method="get">
                    <input type="submit" value="Edit" class="btn btn-warning">
                </form>
                </div>
            </td>
        </tr>
    }
</table>